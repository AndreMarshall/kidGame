<Window x:Class="WPF_Math_Game_Outline.wndHighScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="wndHighScores" Height="500" Width="550" Closing="Window_Closing">
    <Grid x:Name="grdFinalScore" Background="LightSlateGray">
        <Label Content="Final Score" FontSize="20" FontWeight="Bold" Height="45" Foreground="AntiqueWhite" HorizontalAlignment="Left" Margin="10,10,0,0" Name="label1" VerticalAlignment="Top" Width="122" />
        <Button Content="Close Final Score" FontSize="18" Height="45" HorizontalAlignment="Left" Margin="176,415,0,0" Name="cmdCloseHighScores" VerticalAlignment="Top" Width="198" Click="cmdCloseHighScores_Click" FontFamily="Franklin Gothic Medium" />
        <TextBlock x:Name="txtBlckFinalScore" HorizontalAlignment="Left" Background="White" Margin="0,309,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="101" Width="544"/>
        <Label x:Name="lblWinnings" Content="" Foreground="AntiqueWhite" HorizontalAlignment="Left" Margin="196,36,0,0" VerticalAlignment="Top" Width="338" Height="35" FontSize="20" FontStyle="Italic"/>
    </Grid>
</Window>
